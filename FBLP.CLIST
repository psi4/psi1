PROC 2 DSN DEST(C) FLAG(4) LEVEL(I) OPT(3) CODE(K)
CONTROL MSG LIST NOF
    IF &STR(&DEST) EQ &STR(*) THEN DO
        F7 &DSN,S,LEVEL(&LEVEL),NOOBJ,OPT(&OPT),LC(0),NOTERM,+
        FLAG(&FLAG),PRINT(*),CODE(&CODE)
      END
      ELSE DO
        F7 &DSN,S,LEVEL(&LEVEL),SYSPRINT(&DEST),NOOBJ,OPT(&OPT),+
        FLAG(&FLAG),LC(66),NOTERM,CODE(&CODE)
      END
    WRITE LASTCC=&LASTCC
