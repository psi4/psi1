                  YY3B1=0.0D0
                  YZ3B1=0.0D0
                  YXAB1=0.0D0
                  YXAB2=0.0D0
                  YXAB3=0.0D0
                  YXAB4=0.0D0
                  YYAB1=0.0D0
                  YYAB2=0.0D0
                  YYAB3=0.0D0
                  YYAB4=0.0D0
                  YZAB1=0.0D0
                  YZAB2=0.0D0
                  YZAB3=0.0D0
                  YZAB4=0.0D0
                  YXSF1=0.0D0
                  YYSF1=0.0D0
                  YZSF1=0.0D0
                  YXSF2=0.0D0
