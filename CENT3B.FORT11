                    IF(INO .GT. 1) THEN
                      IF(IJ .LE. ITM1) THEN
                        ISPOT=IM1M0 + IJM1*I2M1M0
                        EXCNST=EX(ISPOT)*GT(GSPOT,IROOT)
                        EYCNST=EY(ISPOT)*GU(GSPOT,IROOT)
                        EZCNST=EZ(ISPOT)*GV(GSPOT,IROOT)
                        IF(IKL .LE. KLQNP1) THEN
                          YXA2=YXA2 + EXCNST
                          YYA2=YYA2 + EYCNST
                          YZA2=YZA2 + EZCNST
                        END IF
